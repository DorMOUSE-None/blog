{
  "name": "ffutop",
  "version": "1.0.0",
  "description": "ffutop's blog site",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_PATH=./ next",
    "build": "NODE_PATH=./ next build",
    "build:content": "processmd content/**/*.md --outputDir content --preview 11 --previewDelimiter '<!-- more -->' --summaryOutput content/summary.json",
    "start": "NODE_PATH=./ next start",
    "export": "npm run build && NODE_PATH=./ next export",
    "deploy": "rm -rf node_modules/.cache .next/ out/ && NODE_PATH=./ next build && NODE_PATH=./ next export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages"
  },
  "author": "ffutop",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-sass": "^1.0.1",
    "next": "^8.1.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "js-yaml": "^3.13.1",
    "processmd": "^4.4.2",
    "rehype-katex": "^1.2.0",
    "rehype-stringify": "^5.0.0",
    "remark": "^10.0.1",
    "remark-frontmatter": "^1.3.1",
    "remark-html": "^9.0.0",
    "remark-math": "^1.0.6",
    "remark-parse": "^6.0.3",
    "remark-parse-yaml": "0.0.2",
    "remark-rehype": "^4.0.0",
    "remark-stringify": "^6.0.4",
    "remark-toc": "^5.1.1",
    "to-vfile": "^5.0.2",
    "unified": "^7.1.0",
    "unist-util-visit": "^1.4.0",
    "vfile-reporter": "^5.1.1"
  }
}
